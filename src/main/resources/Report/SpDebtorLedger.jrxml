	<queryString>
		<![CDATA[SELECT s_no, (SELECT SP_NAME FROM m_sp WHERE sp_code=a.s_no) sp_name, trans_type, trans_no, trans_dt, common.to_bs(trans_dt) nep_trans_dt,
item_code,dr_amt, cr_amt, (dr_amt-cr_amt) bal_amt, remarks, create_by, create_dt,
nvl((SELECT sum(total_amt) FROM vw_ledger
where s_no=a.s_no AND trans_dt<$P{pm_frm_dt}
AND post_flag='Y'),0) opening_bal
FROM
(select s_no, trans_type, trans_no, trans_dt, imp_year, imp_month, item_code,
dr_cr_flag, total_amt dr_amt, 0 cr_amt, remarks, create_by, create_dt
from vw_ledger
WHERE s_no=$P{pm_sp_code}
AND trans_dt between $P{pm_frm_dt} and $P{pm_to_dt}
AND dr_cr_flag=1
AND post_flag='Y'
UNION ALL
select s_no, trans_type, trans_no, trans_dt, imp_year, imp_month, item_code,
dr_cr_flag, 0 dr_amt, abs(total_amt) cr_amt, remarks, create_by, create_dt
from vw_ledger
WHERE s_no=$P{pm_sp_code}
AND trans_dt between $P{pm_frm_dt} and $P{pm_to_dt}
AND dr_cr_flag=-1
AND post_flag='Y'
ORDER BY trans_dt, create_dt, dr_cr_flag desc) a]]>
	</queryString>
